program nums;
var
	x, y: integer;
begin
	write('Enter first number: ');
	read(x);
	write('Enter second number: ');
	read(y);
	writeln('x + y = ', x + y);
	writeln('x - y = ', x - y);
	writeln('x * y = ', x * y);
	writeln('x / y = ', x / y);
	writeln('x div y = ', x div y);
	writeln('x mod y = ', x mod y);
end.
